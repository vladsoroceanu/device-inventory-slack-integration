{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "impliedStrict": true
    },
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "eqeqeq": 2,
        "quotes": [
            "off",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-undef": 1,
        "no-unused-vars": 1
    }
}